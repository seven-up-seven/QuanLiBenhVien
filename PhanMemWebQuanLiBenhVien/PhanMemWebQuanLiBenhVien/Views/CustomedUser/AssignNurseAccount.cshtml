@model Nurse

<form method="post">
    <p>ID y tá: @Model.NurseId</p>
    <p>Tên y tá: @Model.NurseName</p>
    <input name="NurseId" asp-for="@Model.NurseId" hidden />
    <input name="username" placeholder="Nhập tên đăng nhập của tài khoản" />
    <input name="password" placeholder="Nhập mật khẩu cho tài khoản" />
    <button type="submit" class="btn-primary">Cấp</button>
</form>

